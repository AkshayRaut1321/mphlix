{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mphlix": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/mphlix",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/CNAME"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/purple-green.css",
              "src/styles.css",
              "src/assets/css/open-iconic-bootstrap.min.css",
              "src/assets/css/animate.css",
              "node_modules/owl.carousel/dist/assets/owl.carousel.min.css",
              "node_modules/owl.carousel/dist/assets/owl.theme.default.min.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/aos.css",
              "src/assets/css/ionicons.min.css",
              "src/assets/css/bootstrap.min.css",
              "node_modules/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css",
              "node_modules/jquery-timepicker/jquery.timepicker.css",
              "src/assets/css/flaticon.css",
              "src/assets/css/icomoon.css",
              "src/assets/css/style.css",
              "src/assets/css/minimal-form/minimal-form.css"
            ],
            "scripts": [
              "src/assets/js/jquery-3.2.1.min.js",
              "src/assets/js/jquery-migrate-3.0.1.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/jquery.easing.1.3.js",
              "src/assets/js/jquery.waypoints.min.js",
              "src/assets/js/jquery.stellar.min.js",
              "node_modules/owl.carousel/dist/owl.carousel.min.js",
              "src/assets/js/jquery.magnific-popup.min.js",
              "src/assets/js/aos.js",
              "src/assets/js/jquery.animateNumber.min.js",
              "node_modules/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js",
              "node_modules/jquery-timepicker/jquery.timepicker.js",
              "src/assets/js/scrollax.min.js",
              "src/assets/js/main.js",
              "src/assets/js/minimal-form/modernizr.custom.js",
              "src/assets/js/minimal-form/classie.js",
              "src/assets/js/minimal-form/stepsForm.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "mphlix:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "mphlix:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "mphlix:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/purple-green.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "mphlix:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "mphlix:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "mphlix"
}